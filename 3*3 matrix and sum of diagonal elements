#include<stdio.h>
#include<conio.h>
void main()
{
int i,j,m,n,a[10][10],sum=0;
clrscr();
printf("Ener m*n matrix:");
scanf("%d%d",&m,&n);
printf("Enter elements of matrix:");
for(i=0;i<m;i++)
{
for(j=0;j<n;j++)
{
scanf("%d",&a[i][j]);
}
}
for(i=0;i<m;i++)
{
sum+=a[i][i];
}
printf("Sum of diagonal elements is %d",sum);
getch();
}
